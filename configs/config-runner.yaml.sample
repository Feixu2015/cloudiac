listen: "0.0.0.0:19030"

runner:
  default_image: "mt5225/tf-ansible:v0.0.1"
  #### 以下配置必须使用绝对路径
  storage_path: "/usr/yunji/cloudiac/storage"
  provider_path: "/usr/yunji/cloudiac/providers"
  plugin_cache_path: "/usr/yunji/cloudiac/plugin-cache"

consul:
  address: "consul.example.com:8500"
  id: "CT-Runner-01"
  ip: "runner01.example.com"
  port: 19030
  tags: "tag1;tag2"
  interval: 5s
  timeout: 3s
  deregister_after: "90m"

log:
  log_level: "debug"
  log_max_days: 7

