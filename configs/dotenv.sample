# 管理员账号密码，只在初始化启动时进行读取
IAC_ADMIN_EMAIL=admin@example.com
IAC_ADMIN_PASSWORD=

# 部署的镜像版本，仅 docker-compose 部署时有效
IAC_VERSION=latest

# IaC 对外提供服务的地址
PORTAL_ADDRESS=http://public.host.ip

# mysql 配置
MYSQL_HOST=mysql
MYSQL_PORT=3306
MYSQL_DATABASE=iac
MYSQL_USER=root
MYSQL_PASSWORD=

# consul 配置
CONSUL_ADDR=private.host.ip:8500

# portal 服务注册信息配置
SERVICE_IP=iac-portal
SERVICE_ID=IaC-Portal-01
SERVICE_TAGS=iac-porta;portal-01
